






















































/* 清除浮动 */
.clearfix:after{
  display:block; 
  content:"clear"; 
  height:0; 
  clear:both; 
  padding-top:5rpx;
  overflow:hidden; 
  visibility:hidden;
}
.fl{
float: left
}
.fr{
float: right
}
image{
  width: 100%;
  height: 100%;
}
.body{
	width: 100%;
	height: 100vh;
	background-color: #EEEEEE;
	overflow: hidden;
}
.news_box{
	width: 684rpx;
	margin: 15rpx auto 0;
	padding: 18rpx;
	background-color: #FFFFFF;
}
.title{
	font-size: 32rpx;
	line-height: 56rpx;
}
.time{
	font-size: 30rpx;
	line-height: 56rpx;
	color: #C0C0C0;
}
.content{
	width: 682rpx;
	font-size: 30rpx;
	border-bottom: 1rpx solid #EEEEEE;
	padding:10rpx 10px 30rpx;
}
.state{
	font-size: 30rpx;
	line-height: 56rpx;
	color: #C0C0C0;
}



