.scorll-list{
	/* box-sizing: border-box; */
	margin-top: 20rpx;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.scroll-box{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	overflow: hidden;
	/* box-sizing: border-box; */
	padding: 0 20rpx;
}
.list-box{
	/* width: 100%; */
	line-height: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	/* justify-content: center; */
}
.ss1{
	width: 18%;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	margin-left: 20rpx;
}
.date{
	margin-top: 20rpx;
	padding: 10rpx 20rpx;
	border-bottom: 2px solid #eee;
}
.name{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 10rpx 20rpx;
}
.name .title{
	font-size: 20px;
}
.right{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 14%;
	color: #666666;
}
.time{
	margin-top: 20rpx;
	width: 100%;
}
.time-list{
	overflow: hidden;
	box-sizing: border-box;
}
.time-list-box{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	box-sizing: border-box;
	height: 150rpx;
}
.week{
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	width: 20%;
	height: 120rpx;
	margin-left: 10rpx;
	background-color: #eee;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.changeWeek{
	background-color: #DCEEFD;
	color: #007AFF;
	border: 1px solid #007AFF;
	width: 22%;
	height: 125rpx;
	border-radius: 10rpx;
}

